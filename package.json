{
  "name": "react-pagi",
  "version": "1.0.0",
  "description": "A simple React pagination component.",
  "main": "dist/index.js",
  "scripts": {
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && webpack-dev-server --mode development",
    "transpile": "babel src -d dist --copy-files",
    "test": "react-scripts test",
    "prepublishOnly": "npm run transpile",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && webpack --mode production",
    "deploy": "gh-pages -d examples/dist",
    "publish-demo": "npm run build && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/husseinIsmail/react-pagi.git"
  },
  "keywords": [
    "pagination",
    "react",
    "react-pagination",
    "react-pagi"
  ],
  "author": "Hussein Ismail",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/husseinIsmail/react-pagi/issues"
  },
  "homepage": "https://github.com/husseinIsmail/react-pagi#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^5.2.7",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^4.5.2",
    "prop-types": "^15.8.1",
    "react-scripts": "^5.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
